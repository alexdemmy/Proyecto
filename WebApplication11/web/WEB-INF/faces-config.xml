<?xml version='1.0' encoding='UTF-8'?>
<faces-config version="2.2"
              xmlns="http://xmlns.jcp.org/xml/ns/javaee"
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd">

    
    <navigation-rule>
		<display-name>persona.xhtml</display-name>
		<from-view-id>/persona.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>registrado</from-outcome>
			<to-view-id>/personas.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>personas.xhtml</display-name>
		<from-view-id>/personas.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>registrarPersona</from-outcome>
			<to-view-id>/persona.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>personas.xhtml</display-name>
		<from-view-id>/personas.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>detallePersona</from-outcome>
			<to-view-id>/detallePersona.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>detallePersona.xhtml</display-name>
		<from-view-id>/detallePersona.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>registrar</from-outcome>
			<to-view-id>/persona.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>detallePersona.xhtml</display-name>
		<from-view-id>/detallePersona.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>registrarAuto</from-outcome>
			<to-view-id>/auto.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>auto.xhtml</display-name>
		<from-view-id>/auto.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>registrado</from-outcome>
			<to-view-id>/personas.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>detallePersona.xhtml</display-name>
		<from-view-id>/detallePersona.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>personas</from-outcome>
			<to-view-id>/autos.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>auto.xhtml</display-name>
		<from-view-id>/auto.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>registroauto</from-outcome>
			<to-view-id>/autos.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
</faces-config>
